<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yuexiao.myrecipe10">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:launchMode="standard"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Initial"
            android:label="@string/title_activity_initial"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".AccountInit" />
        <activity android:name=".SignUp" />
        <activity android:name=".LogIn" />
        <activity android:name=".personal1" />
        <activity android:name=".personal2" />
        <activity android:name=".personal3" />
    </application>

</manifest>
